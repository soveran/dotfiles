. .rcrc-local

fn prompt {
  switch($GS_NAME) {
    case ()
      prompt=('$ ' '')
    case *
      prompt=('$$ ' '')
  }

  if (! ~ $STY ()) {
  	if (~ $DVTM ()) {
    		printf '\033k%s\033\\' `{basename `{pwd}}
	}
  }
}

fn st { git status -sb }
fn gst { tig status }
fn gsh { git stash --keep-index }
fn gsp { git stash pop }
fn gci { git commit $* }
fn gco { git checkout $* }
fn push { git push $* }
fn pull { git pull $* }

fn serve {
  cat $1 | nc -l 8080
}

fn tunnel {
  ssh -ND 9999 $1
}
